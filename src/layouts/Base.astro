---
import BaseHead from '../components/BaseHead.astro';


export interface Props {
  title: string;
}
const { title } = Astro.props;
---

<!doctype html>
<html lang="es">
  <BaseHead title="AstroTip" description="My First Page with AstroMan!" >
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title}</title>
  </head>
  <body class="h-screen">
        <slot />
  </body>
</html>
